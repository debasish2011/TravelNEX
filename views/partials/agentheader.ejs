<style>
  #logo {
    height: 3rem;
  }

  #logo g {
    fill: white
  }

  #travel {
    height: 3rem;

  }

  #travel g {
    fill: #c43030
  }

  #nex {
    height: 2rem;
  }

  #nex g {
    fill: rgba(255, 255, 255, 0.824)
  }

  /* media quaries */
  @media (max-width: 500px) {
    #logo {
      height: 3rem;
    }

    #travel {
      display: none;
    }

    #nex {
      display: none;
    }
  }

  @media (min-width: 501px) and (max-width: 768px) {
    #logo {
      display: none;
    }

    #travel {
      height: 3rem;
    }

    #nex {
      display: 2rem;
    }
  }
</style>

