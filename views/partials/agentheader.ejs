<div id="verify">
  <div class="alert alert-success" role="alert">
    <h4 class="alert-heading">Welcome!</h4>
    <p>Thank you for registering with us. Please verify your email address by clicking the link in the email we just
      sent you.</p>
    <hr>
    <p class="mb-0">Didn't receive the email? <a href="/resend">Click here to resend</a></p>
  </div>
</div>

<% if (agent.verified == true) { %>
  <script>
    document.getElementById('verify').style.display = 'none';
  </script>
<% } %>

<script>
  let url = window.location.href;
  let btnbox = document.getElementById('btnbox');
  if (url.includes('createappointments')) {
    btnbox.innerHTML = `<a class="btn btn-outline-danger my-1" href="/logout">Logout</a>`;
  } else {
    btnbox.innerHTML = `<a class="btn btn-outline-primary my-1" href="/api/appointments/createappointments">Create An Appointment</a>
      <a class="btn btn-outline-danger my-1" href="/logout">Logout</a>`;
  }
</script>